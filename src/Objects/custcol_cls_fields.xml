        <appliestoestimate>T</appliestoestimate>
        <appliestoworkorder>T</appliestoworkorder>
        <colitem>T</colitem>
        <colcomponent>T</colcomponent>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_dim_length">
        <label>Length (ft)</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>3</precision>
        <description>Actual length in feet</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoworkorder>T</appliestoworkorder>
        <colitem>T</colitem>
        <colcomponent>T</colcomponent>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_calculated_bf">
        <label>Calculated BF</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>4</precision>
        <description>Calculated board feet for this line</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoworkorder>T</appliestoworkorder>
        <appliestoinvoice>T</appliestoinvoice>
        <appliestoitemfulfillment>T</appliestoitemfulfillment>
        <colitem>T</colitem>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_conversion_factor">
        <label>Conversion Factor</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>6</precision>
        <description>Factor used to convert display qty to BF</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <colitem>T</colitem>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_bf_unit_cost">
