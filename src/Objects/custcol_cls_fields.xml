        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <colitem>T</colitem>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_extended_bf_cost">
        <label>Extended BF Cost</label>
        <fieldtype>CURRENCY</fieldtype>
