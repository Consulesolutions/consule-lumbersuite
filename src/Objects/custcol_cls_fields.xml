<customcolumnfields>
    <transactioncolumncustomfield scriptid="custcol_cls_selling_uom">
        <label>Selling UOM</label>
        <fieldtype>SELECT</fieldtype>
        <selectrecordtype>[scriptid=customlist_cls_uom_codes]</selectrecordtype>
        <description>Unit of measure for display/selling</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoinvoice>T</appliestoinvoice>
        <appliestoitemfulfillment>T</appliestoitemfulfillment>
        <colitem>T</colitem>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_display_qty">
        <label>Display Qty</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>4</precision>
        <description>Quantity in selling UOM</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoinvoice>T</appliestoinvoice>
        <colitem>T</colitem>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_dim_thickness">
        <label>Thickness (in)</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>3</precision>
        <description>Actual thickness in inches</description>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoworkorder>T</appliestoworkorder>
        <colitem>T</colitem>
        <colcomponent>T</colcomponent>
    </transactioncolumncustomfield>
    <transactioncolumncustomfield scriptid="custcol_cls_dim_width">
        <label>Width (in)</label>
        <fieldtype>DECIMALNUM</fieldtype>
        <precision>3</precision>
        <description>Actual width in inches</description>
