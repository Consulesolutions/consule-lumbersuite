<restletscripts>
    <!-- Report Export RESTlet -->
    <restlet scriptid="customscript_cls_report_export_rl">
        <name>CLS Report Export</name>
        <scriptfile>[/SuiteScripts/ConsuleLumberSuite/reporting/cls_report_export_rl.js]</scriptfile>
        <description>RESTlet for exporting reports in various formats</description>
        <isinactive>F</isinactive>
        <notifyowner>T</notifyowner>
        <scriptdeployments>
            <scriptdeployment scriptid="customdeploy_cls_report_export_rl">
                <status>RELEASED</status>
                <isdeployed>T</isdeployed>
                <allroles>T</allroles>
                <loglevel>DEBUG</loglevel>
            </scriptdeployment>
        </scriptdeployments>
    </restlet>
</restletscripts>
