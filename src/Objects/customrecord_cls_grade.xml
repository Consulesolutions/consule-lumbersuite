<customrecordtype scriptid="customrecord_cls_grade">
    <recordname>CLS Grade</recordname>
    <description>Lumber grade definitions with pricing modifiers</description>
    <isinactive>F</isinactive>
    <includename>T</includename>
    <showid>T</showid>
    <shownotes>T</shownotes>
    <allowquickadd>T</allowquickadd>
    <allowquicksearch>T</allowquicksearch>
    <enablemailmerge>F</enablemailmerge>
    <enablenumbering>F</enablenumbering>
    <allowinlineediting>T</allowinlineediting>
    <allowinlinedeleting>F</allowinlinedeleting>
    <customrecordcustomfields>
        <customrecordcustomfield scriptid="custrecord_cls_grade_code">
            <label>Grade Code</label>
            <fieldtype>TEXT</fieldtype>
            <ismandatory>T</ismandatory>
            <isunique>T</isunique>
            <maxlength>10</maxlength>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_grade_price_mod">
            <label>Price Modifier</label>
            <fieldtype>PERCENT</fieldtype>
            <defaultvalue>100</defaultvalue>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_grade_sort">
            <label>Sort Order</label>
            <fieldtype>INTEGER</fieldtype>
            <defaultvalue>0</defaultvalue>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_grade_desc">
            <label>Description</label>
            <fieldtype>TEXTAREA</fieldtype>
        </customrecordcustomfield>
    </customrecordcustomfields>
</customrecordtype>
