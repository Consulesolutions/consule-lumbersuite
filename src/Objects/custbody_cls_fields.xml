<custombodyfields>
    <transactionbodycustomfield scriptid="custbody_cls_total_bf">
        <label>Total Board Feet</label>
        <fieldtype>FLOAT</fieldtype>
        <description>Total calculated board feet for all lines</description>
        <storevalue>T</storevalue>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
        <appliestoworkorder>T</appliestoworkorder>
        <appliestoitemfulfillment>T</appliestoitemfulfillment>
        <appliestoinvoice>T</appliestoinvoice>
    </transactionbodycustomfield>
    <transactionbodycustomfield scriptid="custbody_cls_conversion_locked">
        <label>Conversion Locked</label>
        <fieldtype>CHECKBOX</fieldtype>
        <defaultchecked>F</defaultchecked>
        <description>Prevent automatic recalculation of BF values</description>
        <storevalue>T</storevalue>
        <appliestosalesorder>T</appliestosalesorder>
        <appliestoestimate>T</appliestoestimate>
    </transactionbodycustomfield>
    <transactionbodycustomfield scriptid="custbody_cls_linked_tally">
        <label>Linked Tally</label>
        <fieldtype>SELECT</fieldtype>
        <selectrecordtype>[scriptid=customrecord_cls_tally]</selectrecordtype>
        <description>Primary tally sheet linked to this transaction</description>
        <storevalue>T</storevalue>
        <appliestoworkorder>T</appliestoworkorder>
    </transactionbodycustomfield>
    <transactionbodycustomfield scriptid="custbody_cls_total_theoretical_bf">
        <label>Total Theoretical BF</label>
        <fieldtype>FLOAT</fieldtype>
        <description>Total theoretical board feet (input)</description>
        <storevalue>T</storevalue>
        <appliestoworkorder>T</appliestoworkorder>
    </transactionbodycustomfield>
    <transactionbodycustomfield scriptid="custbody_cls_total_waste_bf">
        <label>Total Waste BF</label>
        <fieldtype>FLOAT</fieldtype>
        <description>Total waste board feet</description>
        <storevalue>T</storevalue>
        <appliestoworkorder>T</appliestoworkorder>
    </transactionbodycustomfield>
</custombodyfields>
