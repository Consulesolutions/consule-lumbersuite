<customrecordtype scriptid="customrecord_cls_yield_reg">
    <recordname>CLS Yield Register</recordname>
    <description>Records yield and waste data from work order completions</description>
    <isinactive>F</isinactive>
    <includename>F</includename>
    <showid>T</showid>
    <shownotes>T</shownotes>
    <allowquickadd>F</allowquickadd>
    <allowquicksearch>T</allowquicksearch>
    <enablenumbering>F</enablenumbering>
    <allowinlineediting>F</allowinlineediting>
    <allowinlinedeleting>F</allowinlinedeleting>
    <customrecordcustomfields>
        <customrecordcustomfield scriptid="custrecord_cls_yield_wo">
            <label>Work Order</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-30</selectrecordtype>
            <ismandatory>T</ismandatory>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_woc">
            <label>WO Completion</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-175</selectrecordtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_item">
            <label>Item</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-10</selectrecordtype>
            <ismandatory>T</ismandatory>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_theoretical">
            <label>Theoretical BF</label>
            <fieldtype>FLOAT</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_actual">
            <label>Actual BF</label>
            <fieldtype>FLOAT</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_waste">
            <label>Waste BF</label>
            <fieldtype>FLOAT</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_recovery">
            <label>Recovery %</label>
            <fieldtype>PERCENT</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_pct">
            <label>Yield %</label>
            <fieldtype>PERCENT</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_waste_reason">
            <label>Waste Reason</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>[scriptid=customrecord_cls_waste_rsn]</selectrecordtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_operator">
            <label>Operator</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-4</selectrecordtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_date">
            <label>Completion Date</label>
            <fieldtype>DATE</fieldtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_subsidiary">
            <label>Subsidiary</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-117</selectrecordtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_location">
            <label>Location</label>
            <fieldtype>SELECT</fieldtype>
            <selectrecordtype>-103</selectrecordtype>
        </customrecordcustomfield>
        <customrecordcustomfield scriptid="custrecord_cls_yield_notes">
            <label>Notes</label>
            <fieldtype>TEXTAREA</fieldtype>
        </customrecordcustomfield>
    </customrecordcustomfields>
</customrecordtype>
